\ProvidesPackage{uklogo}
\RequirePackage{graphicx}


\newcommand\uklogofont{\sffamily}
%\usepackage{gridset}
%\usepackage{blindtext}
% \raggedbottom

% \pagestyle{myheadings}

%\markright{gridbase=\gridbase, gridinterval=\gridinterval\ without move down}%
\newcommand\uklogo[1][32pt]{%
  \begingroup % changes should be local
  \uklogofont
  \def\uksmallfont{\the\dimexpr #1/4\relax}%
  \def\mystrut{\vrule width 0pt height \baselineskip depth \the\dimexpr #1/15\relax}%
  \def\selectsmall{\fontsize{\uksmallfont}{0pt}\selectfont\mystrut}%
  \def\ukbigfont{\the\dimexpr #1/2\relax}%
  \def\selectbig{\fontsize{\ukbigfont}{0pt}\selectfont\mystrut}%
  \def\uklogoskip{\the\dimexpr #1/10\relax}%
  \typeout{#1, \ukbigfont, \uksmallfont}
  %%%%%%%%%%%
  \def\mybox##1{\hbox{##1}\typeout{baseline: \the\baselineskip}}
  \def\logodim{\the\dimexpr#1\relax}%
  \hbox{%
    \vbox{%
      \hbox{\noindent\includegraphics[height=\logodim,width=\logodim]{logo-5.mps}}%
    }%
    \hskip\uklogoskip%
    \vbox{%
      % we must set the baselineskip outside
      \baselineskip=\uksmallfont
      \mybox{\selectsmall Ústřední}%\\% 
      \baselineskip=\ukbigfont
      \mybox{\selectbig\textbf{knihovna}}%\\%
      \baselineskip=\uksmallfont
      \mybox{\selectsmall PedF UK}
    }%
  }%
  \endgroup
}

\endinput
